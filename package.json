{
  "name": "tello-node",
  "version": "1.0.0",
  "description": "Tello Server",
  "main": "bin/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node ./bin/index.js",
    "test": "mocha ./bin/test/**.js",
    "coverage": "nyc mocha ./bin/test/**.js"
  },
  "keywords": [
    "drone",
    "tello"
  ],
  "author": "Nick Nance",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "lines": 80,
    "exclude": [
      "**/adapters",
      "**/test",
      "src/index.ts"
    ]
  }
